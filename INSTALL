doas make oldconfig -j16 CC=/usr/lib/llvm/18/bin/clang LD=/usr/lib/llvm/18/bin/ld.lld AR=/usr/lib/llvm/18/bin/llvm/18-ar NM=/usr/lib/llvm/18/bin/llvm/18-nm STRIP=/usr/lib/llvm/18/bin/llvm/18-strip OBJCOPY=/usr/lib/llvm/18/bin/llvm/18-objcopy OBJDUMP=/usr/lib/llvm/18/bin/llvm/18-objdump READELF=/usr/lib/llvm/18/bin/llvm/18-readelf HOSTCC=/usr/lib/llvm/18/bin/clang HOSTCXX=/usr/lib/llvm/18/bin/clang++ HOSTAR=/usr/lib/llvm/18/bin/llvm/18-ar HOSTLD=/usr/lib/llvm/18/bin/ld.lld LLVM_IAS=1 LLVM=1

